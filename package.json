{
  "name": "imagemagick-docker",
  "version": "0.1.4",
  "description": "A Node web API to convert, cache, and serve images with ImageMagick",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "DEBUG=app nodemon index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "restartable": "rs",
    "ext": "js,css,ejs",
    "watch": [
      "./"
    ],
    "delay": 1000,
    "env": {
      "PORT": 5100
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "filenamify": "^6.0.0",
    "fs-extra": "^11.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "shell-escape": "^0.2.0"
  }
}
